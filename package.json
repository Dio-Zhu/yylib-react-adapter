{
  "name": "yylib-react-adapter",
  "version": "0.1.0",
  "description": "yylib-react库的适配器",
  "main": "dist/index.js",
  "scripts": {
    "cnpm": "npm config set registry https://registry.npm.taobao.org",
    "cnpm-clear": "npm config delete registry",
    "clean-dist": "rimraf lib dist coverage",
    "build-babel": "babel ./src --out-dir ./dist",
    "build": "npm run clean-dist && npm run build-babel",
    "start": "webpack-dev-server --config webpack.config.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublish": "npm run build"
  },
  "author": "Dio.Zhu",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "babel-cli": "6.10.1",
    "babel-core": "6.9.1",
    "babel-loader": "6.2.4",
    "babel-polyfill": "6.9.1",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-0": "6.24.1",
    "rimraf": "2.5.4",
    "webpack": "1.13.1",
    "webpack-dev-server": "1.14.1"
  },
  "dependencies": {
    "zzj-genadapter":"~0.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Dio-Zhu/yylib-react-adapter.git"
  }
}
